local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()


local Window = OrionLib:MakeWindow({Name = "Title of the library", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})


--Tabs
local Main = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Misc = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})



--locals
local Workspace = game:GetService("Workspace")
local CubeFolder = Workspace.Folder
_G.AutoRolla = false
_G.AutoDropa = false

--function
function AutoRoll()
  while _G.AutoRolla == true do
    game:GetService("ReplicatedStorage").Signals.Button:FireServer()
     wait(0.1)
  end
  
end

function AutoDrop()
  while _G.AutoDropa == true do
    for _, Cube in ipairs(CubeFolder:GetChildren()) do
      print(Cube.Position)
      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Cube.Position)
      wait(0.1)
  end
  wait(0.1)
  end
  
end


function ReloadScript()
  _G.AutoRolla = false
  _G.AutoDropa = false
  loadstring(game:HttpGet('https://raw.githubusercontent.com/Paule1248/IDK/main/Button%20RNG'))()
end




--IDK
Main:AddToggle({
	Name = "AutoRoll",
	Default = false,
	Callback = function(Value)
		_G.AutoRolla = Value
    AutoRoll()
	end    
})

Main:AddToggle({
	Name = "Auto Collect Drops",
	Default = false,
	Callback = function(Value)
		_G.AutoDropa = Value
    AutoDrop()
	end    
})


Misc:AddButton({
	Name = "ReloadScript",
	Callback = function()
      ReloadScript()
  	end    
})
